{
    "name": "ocura/above-the-fold-tracker",
    "type": "wordpress-plugin",
    "require": {
        "php": "^7.4 || ^8.0"
    },
    "require-dev": {
        "php": "^8.0",
        "phpunit/phpunit": "^10.0",
        "mockery/mockery": "^1.0",
        "yoast/phpunit-polyfills": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Ocura\\AboveTheFoldTracker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AboveTheFold\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "platform-check": false,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}